<?xml version='1.0' encoding='utf-8'?>
<odoo>

    <record id="stock_transport_batch_tranfer_tree_view" model="ir.ui.view">
        <field name="name">stock.transport.batch.tranfer.tree.view</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.vpicktree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="after">
                <field name="volume"/>
                <field name="weight"/>
            </xpath>
        </field>
    </record>

</odoo>
